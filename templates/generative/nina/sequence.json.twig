{% set sequence_name = target.name ~ " - Advanced" %}
{# TARGET #}
{% set target_name = target.name %}
{% set position_angle = 0.0 %}

{% set target_ra_float = target.ra %}
{% set ra_hours = target_ra_float | round(0, 'floor') %}
{% set ra_minutes_float = (target_ra_float - ra_hours) * 60 %}
{% set ra_minutes = ra_minutes_float | round(0, 'floor') %}
{% set ra_seconds = (ra_minutes_float - ra_minutes) * 60 %}

{% set target_dec_float = target.dec %}
{% set negative_dec = (target_dec_float < 0)?"true":"false" %}
{% set dec_abs = target_dec_float | abs %}
{% set dec_degrees = dec_abs | round(0, 'floor') %}
{% set dec_minutes_float = (dec_abs - dec_degrees) * 60 %}
{% set dec_minutes = dec_minutes_float | round(0, 'floor') %}
{% set dec_seconds = (dec_minutes_float - dec_minutes) * 60 %}

{# S #}
{% set s_count = 12 %}
{% set s_exposure_time = 300.0 %}
{% set s_gain = 252 %}
{% set s_offset = 15 %}
{# H #}
{% set h_count = 12 %}
{% set h_exposure_time = 300.0 %}
{% set h_gain = 252 %}
{% set h_offset = 15 %}
{# O #}
{% set o_count = 12 %}
